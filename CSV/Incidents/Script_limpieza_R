library(readxl)

## Alabama

incidents <-readxl::read_excel("C:/Users/Usuario/Desktop/datos/incidentsalabama/Incidents_Offenses_Victims_and_Known_Offenders_by_Offense_Category_2022.xlsx")
head(incidents)
incidents <- as.data.frame(incidents)
incidents
colnames(incidents) = c("Offense Category", "Incidents", "Offenses", "Victims", "Known offenders")

incidents <- incidents[-c(1:2), ]
incidents <- head(incidents, n = nrow(incidents) - 3)

incidents$estado <- "Alabama"


#Alaska

alaska <-readxl::read_excel("C:/Users/Usuario/Desktop/datos/incidentsalaska/Incidents_Offenses_Victims_and_Known_Offenders_by_Offense_Category_2022.xlsx")
alaska <- as.data.frame(alaska)
colnames(alaska) = c("Offense Category", "Incidents", "Offenses", "Victims", "Known offenders")

alaska <- alaska[-c(1:2), ]
alaska <- head(alaska, n = nrow(alaska) - 3)

alaska$estado <- "Alaska"
incidents <- rbind(incidents, alaska)

